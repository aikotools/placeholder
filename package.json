{
  "name": "@aikotools/placeholder",
  "version": "0.0.0-semantic-release",
  "description": "Advanced placeholder template engine with generate and compare modes",
  "type": "module",
  "main": "./dist/aikotools-placeholder.js",
  "module": "./dist/aikotools-placeholder.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/aikotools-placeholder.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{js,ts,json}\"",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "depcheck": "depcheck",
    "build": "npm run format && npm run lint && npm run depcheck && tsc && vite build",
    "test": "npm run build && vitest run --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "docs:build": "bash scripts/build-readme.sh"
  },
  "keywords": [
    "template",
    "placeholder",
    "testing",
    "json",
    "xml",
    "matcher"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "luxon": "^3.4.4",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.5",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.3.3",
    "vite": "^7.1.10",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.2.0"
  }
}
