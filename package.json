{
  "name": "@aikotools/placeholder",
  "version": "0.0.0-semantic-release",
  "description": "Advanced placeholder template engine with generate and compare modes",
  "type": "module",
  "main": "./dist/aikotools-placeholder.cjs",
  "module": "./dist/aikotools-placeholder.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/aikotools-placeholder.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/aikotools-placeholder.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aikotools/placeholder.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{js,ts,json}\"",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "depcheck": "depcheck",
    "build": "npm run format && npm run lint && npm run depcheck && tsc && vite build",
    "test": "npm run build && vitest run --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "docs:build": "bash scripts/build-readme.sh"
  },
  "keywords": [
    "template",
    "placeholder",
    "testing",
    "json",
    "xml",
    "matcher"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "luxon": "^3.7.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.2.3",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "depcheck": "^1.4.7",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18"
  }
}
